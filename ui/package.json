{
  "name": "@oroszgy/ag-ui-deepchat-vue",
  "version": "0.2.0",
  "type": "module",
  "description": "A Vue.js chat component built on deep-chat and AG-UI client",
  "main": "dist/index.js",
  "module": "dist/index.esm.js",
  "types": "dist/index.d.ts",
  "files": [
    "dist"
  ],
  "scripts": {
    "build": "rollup -c",
    "build:types": "vue-tsc --declaration --emitDeclarationOnly --outDir dist",
    "dev": "rollup -c -w",
    "serve": "parcel index.html --open",
    "serve:prod": "parcel build index.html --dist-dir demo-dist",
    "prepublishOnly": "npm run build"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/oroszgy/agui-deepchat-integration.git"
  },
  "publishConfig": {
    "registry": "https://npm.pkg.github.com"
  },
  "keywords": [
    "vue",
    "typescript",
    "chat",
    "deep-chat",
    "ag-ui",
    "component"
  ],
  "author": "oroszgy",
  "license": "MIT",
  "peerDependencies": {
    "vue": "^3.3.0"
  },
  "dependencies": {
    "@ag-ui/client": "^0.0.35",
    "@bufbuild/protobuf": "^1.10.0"
  },
  "devDependencies": {
    "@rollup/plugin-commonjs": "^25.0.7",
    "@rollup/plugin-node-resolve": "^15.2.3",
    "@rollup/plugin-typescript": "^11.1.6",
    "@vue/compiler-sfc": "^3.5.17",
    "@vitejs/plugin-vue": "^5.0.0",
    "rollup": "^4.9.6",
    "rollup-plugin-esbuild": "^6.1.1",
    "rollup-plugin-vue": "^6.0.0",
    "rollup-plugin-postcss": "^4.0.2",
    "typescript": "^5.0.0",
    "vue": "^3.3.0",
    "vue-tsc": "^1.8.0",
    "esbuild": "^0.19.0",
    "parcel": "^2.9.0",
    "@parcel/transformer-vue": "^2.9.0",
    "@parcel/optimizer-terser": "^2.15.4"
  }
}
